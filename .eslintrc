{
  "env": {"browser": true},
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["babel"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": ["off"],
    "import/prefer-default-export": ["off"],
    "jsx-a11y/no-static-element-interactions": ["off"],
    "no-console": ["error", { allow: ["warn"] }],
    "semi": ["error", "never"],
    "max-len": ["error", 80]
  }
}
